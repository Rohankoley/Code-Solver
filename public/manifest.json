{
    "manifest_version":3,
    "version":"1.0.0",
    "name":"Code-Solver",
    "action":{
        "default_popup":"index.html",
        "default_icon": "gpt.png"
    },
    "icons":{
        "128":"gpt.png"
    },

    "permissions":[
        "scripting",
        "tabs",
        "activeTab"
    ],
    "host_permissions":[
        "http://*/*",
        "http://*/*"
    ]
}